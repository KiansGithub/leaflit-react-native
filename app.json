{
  "expo": {
    "name": "leaflit",
    "slug": "leaflit",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "buildNumber": "1.0.20",
      "supportsTablet": true,
      "bundleIdentifier": "com.leaflit.kian",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Leaflit uses your location to help you select a job location and assign leafleting jobs based on your current location.",
        "NSCameraUsageDescription": "Leaflit requires camera access to allow you to take photos for job verification and completion.",
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "versionCode": 17,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "INTERNET",
        "VIBRATE"
      ],
      "package": "com.leaflit.kian",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBOaSEr6ei6WrozkQMoABpV5Yd9mUeuKf8"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "jsEngine": "hermes",
    "scheme": "leaflit",
    "platforms": [
      "ios",
      "android"
    ],
    "extra": {
      "eas": {
        "projectId": "cbfc8d44-0270-413a-9195-d16843a5dbaf"
      },
      "googleMapsApiKey": "AIzaSyBOaSEr6ei6WrozkQMoABpV5Yd9mUeuKf8"
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "sdkVersion": "51.0.0",
    "plugins": [
      "expo-secure-store"
    ]
  }
}
